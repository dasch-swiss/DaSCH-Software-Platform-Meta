{
  "release-type": "rust",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Enhancements",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "Maintenance",
      "hidden": false
    },
    {
      "type": "build",
      "section": "Maintenance",
      "hidden": false
    },
    {
      "type": "docs",
      "section": "Documentation",
      "hidden": false
    },
    {
      "type": "data",
      "section": "Project Data",
      "hidden": false
    },
    {
      "type": "refactor",
      "section": "Maintenance",
      "hidden": false
    }
  ],
  "plugins": ["sentence-case"],
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "rust",
      "bump-minor-pre-major": true,
      "bump-patch-for-minor-pre-major": true,
      "draft": true,
      "prerelease": false
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
